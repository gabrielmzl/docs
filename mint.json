{
  "$schema": "https://mintlify.com/schema.json",
  "name": "Steamwebapi",
  "logo": {
    "dark": "/logo/steamwebapi-logo.png",
    "light": "/logo/steamwebapi-logo.png"
  },
  "favicon": "/favicon.png",
  "colors": {
    "primary": "#06BCEE",
    "light": "#81D7F7",
    "dark": "#06A6D1",
    "background": {
      "dark": "#111111"
    }
  },
  "topbarCtaButton": {
    "name": "Dashboard",
    "url": "https://steamwebapi.com/dashboard"
  },
  "tabs": [
    {
      "name": "API Reference",
      "url": "api-reference"
    }
  ],
  "anchors": [
    {
      "name": "Documentation",
      "icon": "book-open-cover",
      "url": "https://steamwebapi.com/docs"
    },
    {
      "name": "Community",
      "icon": "discord",
      "url": "https://discord.gg/N5yWqRYzEr"
    }
  ],
  "navigation": [
    {
      "group": "Get Started",
      "pages": ["introduction", "quickstart"]
    },
    {
      "group": "Packages",
      "pages": ["packages/nodejs", "packages/python", "packages/php"]
    },
    {
      "group": "Example projects",
      "pages": ["projects/steam-inventory-calculator"]
    },
    {
      "group": "API Documentation",
      "pages": ["api-reference/authentication"]
    },
    {
      "group": "Currency Endpoint",
      "pages": [
        "api-reference/currency/list-all",
        "api-reference/currency/exchange-rate"
      ]
    },
    {
      "group": "Explore Endpoint",
      "pages": [
        "api-reference/explore/random-profiles",
        "api-reference/explore/last-profiles",
        "api-reference/explore/top-list"
      ]
    },
    {
      "group": "Float Endpoint",
      "pages": [
        "api-reference/float/get-float"
      ]
    },
    {
      "group": "Items Endpoint",
      "pages": [
        "api-reference/items/get-all-items-from-game",
        "api-reference/items/get-item-info-from-game",
        "api-reference/items/search-for-items",
        "api-reference/items/get-item-details",
        "api-reference/items/get-price-history-from-item"
      ]
    },
    {
      "group": "Inventory Endpoint",
      "pages": [
        "api-reference/inventory/get-inventory",
        "api-reference/inventory/track-inventory"
      ]
    },
    {
      "group": "Profile Endpoint",
      "pages": [
        "api-reference/profile/convert-steamid",
        "api-reference/profile/get-user-profile",
        "api-reference/profile/get-friendlist"
      ]
    }
  ],
  "modeToggle": {
    "default": "dark",
    "isHidden": true
  },
  "backgroundImage": "/images/background.png",
  "footerSocials": {
    "discord": "https://discord.gg/N5yWqRYzEr",
    "github": "https://github.com/steamwebapi"
  }
}
