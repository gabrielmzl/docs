{
  "$schema": "https://mintlify.com/schema.json",
  "name": "Steamwebapi",
  "logo": {
    "dark": "/logo/steamwebapi-logo.png",
    "light": "/logo/steamwebapi-logo.png"
  },
  "favicon": "/favicon.png",
  "colors": {
    "primary": "#06BCEE",
    "light": "#81D7F7",
    "dark": "#06A6D1",
    "background": {
      "dark": "#111111"
    }
  },
  "topbarCtaButton": {
    "name": "Dashboard",
    "url": "https://steamwebapi.com/dashboard"
  },
  "tabs": [
    {
      "name": "API Reference",
      "url": "api-reference"
    }
  ],
  "anchors": [
    {
      "name": "Documentation",
      "icon": "book-open-cover",
      "url": "https://steamwebapi.com/docs"
    },
    {
      "name": "Community",
      "icon": "discord",
      "url": "https://discord.gg/N5yWqRYzEr"
    }
  ],
  "navigation": [
    {
      "group": "Get Started",
      "pages": ["introduction", "quickstart"]
    },
    {
      "group": "Packages",
      "pages": ["packages/nodejs", "packages/python", "packages/php"]
    },
    {
      "group": "Example projects",
      "pages": ["projects/steam-inventory-calculator"]
    },
    {
      "group": "API Documentation",
      "pages": ["api-reference/authentication"]
    },
    {
      "group": "Endpoint Examples",
      "pages": [
        "api-reference/endpoint/get",
        "api-reference/endpoint/create",
        "api-reference/endpoint/update",
        "api-reference/endpoint/delete"
      ]
    }
  ],
  "modeToggle": {
    "default": "dark",
    "isHidden": true
  },
  "backgroundImage": "/images/background.png",
  "footerSocials": {
    "discord": "https://discord.gg/N5yWqRYzEr",
    "github": "https://github.com/steamwebapi"
  }
}
